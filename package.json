    "@slack/bolt": "^3.3.0",
    "@vendia/serverless-express": "^4.3.4",
    "aws-lambda": "^1.0.6",
    "aws-sdk": "^2.907.0",
    "aws-xray-sdk": "^3.3.3",
    "dayjs": "^1.10.4",
    "logger": "git+https://github.com/boristane/logger.git"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.76",
